---
layout : layouts/base.njk
---

		<!-- Header -->
		<section id="header">
			<header class="major">
				<h1>
					<a href="{{ data.product }}">{{ data.app_title }}</a>
				</h1>
				<p>
					<a href="https://appseed.us/static-site/">Static Site</a> prototyped on top of <a href="https://www.11ty.io/">Eleventy SSG</a>.
					<br />
					<a href="{{ data.github }}">Sources</a> - <a href="/elements/">UI Kit</a>  
				</p>
			</header>
			<div class="container">
				<ul class="actions special">
					<li><a href="#one" class="button primary scrolly">Begin</a></li>
				</ul>
			</div>
		</section>

		<!-- One -->
		<section id="one" class="main special">
			<div class="container">
				<span class="image fit primary"><img src="images/pic01.jpg" alt="" /></span>
